{
  "name": "flatohh",
  "version": "1.0.8",
  "description": "Lightweight utility to deeply flatten and unflatten nested JavaScript objects and arrays using dot and bracket notation",
  "keywords": [
    "flatten",
    "unflatten",
    "deflatten",
    "deep-flat",
    "object",
    "nested",
    "dot-notation",
    "bracket-notation",
    "typescript",
    "utility",
    "form-data",
    "zero-dependency",
    "esm",
    "cjs",
    "commonjs"
  ],
  "homepage": "https://github.com/311ecode/flatohh",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/311ecode/flatohh.git"
  },
  "bugs": {
    "url": "https://github.com/311ecode/flatohh/issues"
  },
  "author": "Imre Toth",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist coverage",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --target es5 --sourcemap --no-splitting",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test",
    "release:patch": "npm run prepublishOnly && npm version patch --no-git-tag-version && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/core": "^1.7.26",
    "@types/node": "^22.7.4",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
